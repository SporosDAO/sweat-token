{
  "name": "sweat-token-frontend",
  "version": "0.1.0",
  "license": "Apache-2.0",
  "private": true,
  "proxy": "http://backend:3001",
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@hookform/error-message": "^2.0.0",
    "@kalidao/hooks": "^0.0.5",
    "@mui/icons-material": "^5.6.2",
    "@mui/material": "^5.7.0",
    "@mui/styles": "^5.8.3",
    "@mui/x-date-pickers": "^5.0.0-beta.6",
    "@openapitools/openapi-generator-cli": "^2.5.1",
    "@rainbow-me/rainbowkit": "^0.6.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.1.0",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "ethers": "^5.6.9",
    "graphql": "^16.5.0",
    "graphql-request": "^4.3.0",
    "moment": "^2.29.4",
    "prettier": "^2.6.2",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-error-overlay": "^6.0.11",
    "react-hook-form": "^7.34.1",
    "react-query": "^3.39.2",
    "react-router-dom": "6",
    "react-scripts": "5.0.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.4.2",
    "wagmi": "^0.6.7",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "apigen": "openapi-generator-cli generate -i http://localhost:3001/api/docs-json -g typescript-axios -o src/api/openapi",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --watchAll=false --detectOpenHandles --forceExit --coverage",
    "test:dev": "react-scripts test --detectOpenHandles",
    "eject": "react-scripts eject",
    "lint": "eslint './src/**/*.js' './src/**/*.ts*' --fix",
    "prepare": "cd ../ && husky install ./frontend/.husky",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "//": "See https://github.com/facebook/create-react-app/issues/11773",
    "react-error-overlay": "6.0.11"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.5.12",
    "@storybook/addon-essentials": "^6.5.12",
    "@storybook/addon-interactions": "^6.5.12",
    "@storybook/addon-links": "^6.5.12",
    "@storybook/builder-webpack5": "^6.5.12",
    "@storybook/manager-webpack5": "^6.5.12",
    "@storybook/node-logger": "^6.5.12",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.12",
    "@storybook/testing-library": "^0.0.13",
    "babel-plugin-named-exports-order": "^0.0.2",
    "eslint-plugin-storybook": "^0.6.4",
    "husky": "^8.0.1",
    "prop-types": "^15.8.1",
    "webpack": "^5.74.0"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn lint"
    }
  },
  "jest": {
    "coverageThreshold": {
          "global": {
            "branches": 8,
            "functions": 8,
            "lines": 8,
            "statements": 8
          }
      }
    }
}
